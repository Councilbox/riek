    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "eslint": "^4.13.1",
    "eslint-plugin-react": "^7.5.1",
    "highlight.js": "^9.1.0",
