{
  "name": "riek",
  "version": "1.0.7",
  "description": "React Inline Edit Kit",
  "author": "Kai Vik <kvi@ieee.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kaivi/riek.git"
  },
  "main": "lib/index.js",
  "keywords": [
    "react",
    "in-place",
    "edit",
    "editor",
    "inplace",
    "inline"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build-lib": "rm -rf lib/*; babel src --out-dir lib",
    "build-demo": "rm demo/demo.js; webpack demo/demo.jsx demo/demo.js",
    "build": "npm run build-lib && npm run build-demo",
    "test": "jest"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "classnames": "^2.2.5",
    "highlight.js": "^9.1.0",
    "react-highlight": "^0.6.1",
    "webpack": "^1.12.11",
    "eslint": "^4.13.1",
    "jest": "^22.0.4",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "react-test-renderer":"^15.5.*"
  },
  "dependencies": {
    "react": "^15.5.*",
    "react-dom": "15.5.*"
  },
  "jest": {
    "verbose": true,
    "coverageDirectory": "./",
    "moduleFileExtensions": [
      "js"
    ],
    "testRegex": "/.*\\.test\\.(js)$"
  }
}
