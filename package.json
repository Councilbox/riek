  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/attently/riek.git"
  },
  "main": "lib/index.js",
  "keywords": [
    "react",
    "in-place",
    "edit",
    "editor",
    "inplace",
    "inline"
  ],
  "bugs": {
    "url": "https://github.com/attently/riek/issues"
  },
  "homepage": "https://github.com/attently/riek#readme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build",
